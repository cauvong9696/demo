<table class="table table-stripped">
  <thead>
    <th><%= t "id" %></th>
    <th><%= t "title" %></th>
    <th><%= t "content" %></th>
    <th></th>
    <th></th>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post["id"] %></td>
      <td><%= post["title"] %></td>
      <td><%= post["content"] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
